    <script>
        const MODULE_ID = 'vectorial-plano';
        const MODULE_TITLE = 'Tema 6: GeometrÃ­a Vectorial del Plano';

        document.addEventListener('DOMContentLoaded', function() {
            Components.createHeader(MODULE_TITLE, '../index.html');
            
            const tabs = [
                { id: 'teoria', label: 'TeorÃ­a', icon: 'ğŸ“š' },
                { id: 'ejemplos', label: 'Ejemplos', icon: 'ğŸ’¡' },
                { id: 'herramientas', label: 'Herramientas', icon: 'ğŸ› ï¸' },
                { id: 'practica', label: 'PrÃ¡ctica', icon: 'âœï¸' },
                { id: 'resumen', label: 'Resumen', icon: 'ğŸ“‹' }
            ];
            Components.createTabs(tabs, 'teoria');
            Components.createScoreDisplay();
            
            loadExercises();
            Gamification.updateDisplay(MODULE_ID);
        });

        const exercises = [
            { id: 1, question: 'Calcular |vâƒ—| si vâƒ— = (3, 4)', correct: '5', hint: '|vâƒ—| = âˆš(3Â² + 4Â²) = âˆš25 = 5' },
            { id: 2, question: 'Si vâƒ— = (2, 1) y uâƒ— = (3, 4), Â¿cuÃ¡nto vale vâƒ—Â·uâƒ—?', correct: '10', hint: 'vâƒ—Â·uâƒ— = 2Ã—3 + 1Ã—4 = 6 + 4 = 10' },
            { id: 3, question: 'Â¿Son perpendiculares vâƒ— = (1, 2) y uâƒ— = (4, -2)? (1=SÃ, 0=NO)', correct: '1', hint: 'vâƒ—Â·uâƒ— = 1Ã—4 + 2Ã—(-2) = 4-4 = 0 â†’ SÃ son perpendiculares' },
            { id: 4, question: 'Si vâƒ— = (6, 8), Â¿cuÃ¡nto vale |vâƒ—|?', correct: '10', hint: '|vâƒ—| = âˆš(6Â² + 8Â²) = âˆš(36+64) = âˆš100 = 10' },
            { id: 5, question: 'Calcular vâƒ— + uâƒ— si vâƒ— = (1, 2) y uâƒ— = (3, 4). Componente x:', correct: '4', hint: 'Suma componente x: 1 + 3 = 4' },
            { id: 6, question: 'Si vâƒ— = (2, 3), Â¿cuÃ¡nto vale 3vâƒ—? Componente y:', correct: '9', hint: '3vâƒ— = (3Ã—2, 3Ã—3) = (6, 9). Componente y = 9' },
            { id: 7, question: 'Distancia de P(0,0) a recta x + y - 2 = 0 (âˆš2 â‰ˆ 1.41)', correct: '1.41', hint: 'd = |0+0-2|/âˆš(1Â²+1Â²) = 2/âˆš2 = âˆš2 â‰ˆ 1.41' },
            { id: 8, question: 'Â¿Son paralelos vâƒ— = (2, 4) y uâƒ— = (1, 2)? (1=SÃ, 0=NO)', correct: '1', hint: 'vâƒ— = 2uâƒ—, son proporcionales â†’ SÃ son paralelos' },
            { id: 9, question: 'Si |vâƒ—| = 5 y vâƒ— = (3, y), Â¿cuÃ¡nto vale y? (positivo)', correct: '4', hint: 'âˆš(3Â²+yÂ²) = 5 â†’ 9+yÂ² = 25 â†’ yÂ² = 16 â†’ y = 4' },
            { id: 10, question: 'Calcular vâƒ— - uâƒ— si vâƒ— = (5, 3) y uâƒ— = (2, 1). Componente x:', correct: '3', hint: 'vâƒ—-uâƒ— = (5-2, 3-1) = (3, 2). Componente x = 3' }
        ];

        function loadExercises() {
            const container = document.getElementById('exercises-container');
            container.innerHTML = exercises.map(ex => `
                <div class="exercise-card" data-exercise="${ex.id}">
                    <h4>Ejercicio ${ex.id}</h4>
                    <p>${ex.question}</p>
                    <input type="number" step="any" id="ex${ex.id}" class="input-field" placeholder="Tu respuesta">
                    <div class="button-group">
                        <button class="btn btn-primary" onclick="checkAnswer(${ex.id})">Verificar</button>
                        <button class="btn btn-secondary" onclick="showHint(${ex.id})">ğŸ’¡ Pista</button>
                    </div>
                    <div id="hint${ex.id}" class="hint-box" class="display-none">${ex.hint}</div>
                    <div id="feedback${ex.id}" class="feedback"></div>
                </div>
            `).join('');
        }

        function checkAnswer(exerciseId) {
            const exercise = exercises.find(ex => ex.id === exerciseId);
            const input = document.getElementById('ex' + exerciseId);
            const feedback = document.getElementById('feedback' + exerciseId);
            const userAnswer = parseFloat(input.value.trim());

            if (isNaN(userAnswer)) {
                feedback.className = 'feedback error';
                feedback.textContent = 'Por favor, ingresa una respuesta numÃ©rica.';
                feedback.style.display = 'block';
                return;
            }

            const correctValue = parseFloat(exercise.correct);
            const isCorrect = Math.abs(userAnswer - correctValue) <= 0.01;
            
            if (isCorrect) {
                feedback.className = 'feedback success';
                feedback.textContent = 'Â¡Correcto! âœ¨ +10 puntos';
                input.disabled = true;
                Gamification.completeExercise(MODULE_ID, true);
            } else {
                feedback.className = 'feedback error';
                feedback.textContent = 'Incorrecto. Intenta de nuevo o usa la pista.';
                Gamification.completeExercise(MODULE_ID, false);
            }
            
            feedback.style.display = 'block';
            Gamification.updateDisplay(MODULE_ID);
        }

        function showHint(exerciseId) {
            const hint = document.getElementById('hint' + exerciseId);
            hint.style.display = hint.style.display === 'none' ? 'block' : 'none';
        }

        // Interactive tools
        function calculateModulo() {
            const vectorStr = document.getElementById('modulo-vector').value;
            const result = document.getElementById('modulo-result');
            
            if (!vectorStr) {
                result.textContent = 'âš ï¸ Por favor, ingresa el vector';
                return;
            }
            
            const [vx, vy] = vectorStr.split(',').map(n => parseFloat(n.trim()));
            
            if (isNaN(vx) || isNaN(vy)) {
                result.textContent = 'âš ï¸ Formato incorrecto. Usa: vx,vy';
                return;
            }
            
            const modulo = Math.sqrt(vx*vx + vy*vy);
            
            result.innerHTML = `
                <strong>Vector:</strong> vâƒ— = (${vx}, ${vy})<br>
                <strong>CÃ¡lculo:</strong> |vâƒ—| = âˆš(${vx}Â² + ${vy}Â²) = âˆš${vx*vx + vy*vy}<br>
                <strong>MÃ³dulo:</strong> |vâƒ—| = ${modulo.toFixed(2)}
            `;
        }

        function calculateDotProduct() {
            const vStr = document.getElementById('dot-v').value;
            const uStr = document.getElementById('dot-u').value;
            const result = document.getElementById('dot-result');
            
            if (!vStr || !uStr) {
                result.textContent = 'âš ï¸ Por favor, ingresa ambos vectores';
                return;
            }
            
            const [vx, vy] = vStr.split(',').map(n => parseFloat(n.trim()));
            const [ux, uy] = uStr.split(',').map(n => parseFloat(n.trim()));
            
            if (isNaN(vx) || isNaN(vy) || isNaN(ux) || isNaN(uy)) {
                result.textContent = 'âš ï¸ Formato incorrecto. Usa: vx,vy';
                return;
            }
            
            const dotProduct = vx*ux + vy*uy;
            const isPerpendicular = Math.abs(dotProduct) < 0.001;
            
            result.innerHTML = `
                <strong>Vectores:</strong> vâƒ— = (${vx}, ${vy}), uâƒ— = (${ux}, ${uy})<br>
                <strong>CÃ¡lculo:</strong> vâƒ—Â·uâƒ— = ${vx}Ã—${ux} + ${vy}Ã—${uy} = ${vx*ux} + ${vy*uy}<br>
                <strong>Producto escalar:</strong> vâƒ—Â·uâƒ— = ${dotProduct}<br>
                ${isPerpendicular ? '<strong>âœ“ Son perpendiculares</strong>' : ''}
            `;
        }

        function calculateDistance() {
            const pointStr = document.getElementById('dist-point').value;
            const lineStr = document.getElementById('dist-line').value;
            const result = document.getElementById('dist-result');
            
            if (!pointStr || !lineStr) {
                result.textContent = 'âš ï¸ Por favor, completa todos los campos';
                return;
            }
            
            const [x0, y0] = pointStr.split(',').map(n => parseFloat(n.trim()));
            const [A, B, C] = lineStr.split(',').map(n => parseFloat(n.trim()));
            
            if ([x0,y0,A,B,C].some(isNaN)) {
                result.textContent = 'âš ï¸ Formato incorrecto';
                return;
            }
            
            const distance = Math.abs(A*x0 + B*y0 + C) / Math.sqrt(A*A + B*B);
            
            result.innerHTML = `
                <strong>Punto:</strong> P(${x0}, ${y0})<br>
                <strong>Recta:</strong> ${A}x + ${B}y + ${C} = 0<br>
                <strong>CÃ¡lculo:</strong> d = |${A}Ã—${x0} + ${B}Ã—${y0} + ${C}| / âˆš(${A}Â² + ${B}Â²)<br>
                <strong>Distancia:</strong> d = ${distance.toFixed(2)} unidades
            `;
        }
    </script>
